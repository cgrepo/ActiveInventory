<table id='tblPower' class="table table-striped table-hover ">
    <thead>
        <th>EQUIPO</th>
        <th>ID</th>
        <th>SERIE</th>
        <th>ACTIVO</th>
        <th colspan='1'></th>
    </thead>
    <tbody>
        <% @screens.each do |sc| %>
            <tr>
                <td>MONITOR</td>
                <td><%= sc.id %></td>
                <td><%= sc.nserie %></td>
                <td><%= sc.ninventary %></td>
                <td>
                    <%= link_to 'Baja Equipo', eqKill_brokens_path(id:sc.id,type:'SCREEN'), remote:true, :class=>"btn btn-danger glyphicon glyphicon-fire" %>
                </td>
            </tr>
        
        <% end %>
    </tbody>
</table>
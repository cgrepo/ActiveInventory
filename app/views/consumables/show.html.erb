<p id="notice"><%= notice %></p>
<div class="container large-padding be-gray ">
	<div class="row ">
		<div class="col-xs-4 col-md-4 col-md-offset-1">
			<p><strong>Tipo:</strong><br>
					<%= @consumable.genus %></p>
			<p><strong>Modelo:</strong><br>
			<%= @consumable.model %></p>
			<% if @consumable.Copier %>
				<p><strong>COPIADORA</strong><br>
				<%= @consumable.Copier.model %></p>
			<% end %>
			<% if @consumable.Printer %>
				<p><strong>"IMPRESORA"</strong><br>
				<%= @consumable.Printer.model %></p>
			<% end %>
		</div>
	</div>
</div>
<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <span class="pull-right">
        <br>
				<%= link_to 'Editar', edit_consumable_path(@consumable), class:'btn btn-default btn-lg' %> |
				<%= link_to 'Regresar', consumables_path, class:'btn btn-default btn-lg' %>
      </span>
    </div>
  </div>
</div>
<p id="notice"><%= notice %></p>
<div class="container-fluid">
  <div class="row">
    <div class="col-xs">
      <div class="box">
        <strong>Numero de Inventario:</strong>
        <%= @printer.ninventary %>
      </div>
      <div class="box">
        <strong>Numero de Serie:</strong>
        <%= @printer.nserie %>
      </div>
      <div class="box">
        <strong>Marca:</strong>
        <%= @printer.brand %>
      </div>
      <div class="box">
         <strong>Modelo:</strong>
        <%= @printer.model %>
      </div>

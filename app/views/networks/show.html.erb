<p id="notice"><%= notice %></p>
<div class="container large-padding be-gray">
	<div class="row ">
    <div class="col-xs  large-padding">     
      	<strong>Tipo Enlace:</strong>
	  		<%= @network.linktype %>
    </div>
  </div>
  <div class="row">
    <div class="col-xs  large-padding">      
      	<strong>Locacion:</strong>
	  		<%= @network.location %>
    </div>
  </div>
  <div class="row">
    <div class="col-xs  large-padding">      
      	<strong>Rango IP:</strong>
	  		<%= @network.iprange %>
    </div>
  </div>
  <div class="row">
    <div class="col-xs  large-padding">      
      	<strong>IP Gateway:</strong>
	  		<%= @network.ip %>
    </div>
  </div>
  <% if @network.wifirouter %>
    <div class="row">
      <div class="col-xs  large-padding">      
          <strong>SSID:</strong>
          <%= @network.ssid_name %>
      </div>
    </div>
  <% end %>
</div>
<br>
<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <span class="pull-right">
        <%= link_to 'Editar', edit_network_path(@network), class:'btn btn-default  btn-lg' %> |
        <%= link_to 'Regresar', networks_path, class:'btn btn-default  btn-lg' %>
      </span>
    </div>
  </div>
</div>
<p id="notice"><%= notice %></p>

<h1>Listado de Computadoras</h1>

<table>
  <thead>
    <tr>
      <th>No Inventario</th>
      <th>No Serie</th>
      <th>Marca</th>
      <th>Modelo</th>
      <th>ID factura</th>
      <th>Fecha compra</th>
      <th>Tipo</th>
      <th>Processador</th>
      <th>Disco Duro</th>
      <th>Memoria</th>
      <th>Bluetooth</th>
      <th>Sistema Operativo</th>
      <th>Office ver.</th>
      <th>Usuarios</th>
      <th>Nombre Pc</th>
      <th>Workgroup</th>
      <th>Wifi</th>
      <th>Mac_Lan</th>
      <th>Ip_Lan</th>
      <th>Mask_Lan</th>
      <th>Mac_wifi</th>
      <th>Ip_wifi</th>
      <th>Mask_wifi</th>
      <th>Notas</th>
      <!--
      <th>Operational</th>
      <th>Reazon</th>
    -->
      <th>Network</th>
      <th>Dependency</th>
      <th>Worker</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @computers.each do |computer| %>
      <tr>
        <td><%= computer.ninventary %></td>
        <td><%= computer.nserie %></td>
        <td><%= computer.brand %></td>
        <td><%= computer.model %></td>
        <td><%= computer.nfactura %></td>
        <td><%= computer.buy_date %></td>
        <td><%= computer.genus %></td>
        <td><%= computer.processor %></td>
        <td><%= computer.hd %></td>
        <td><%= computer.memory %></td>
        <td><%= computer.bluetooth %></td>
        <td><%= computer.os %></td>
        <td><%= computer.voffice %></td>
        <td><%= computer.users %></td>
        <td><%= computer.name %></td>
        <td><%= computer.workgroup %></td>
        <td><%= computer.wifi %></td>
        <td><%= computer.maclan %></td>
        <td><%= computer.iplan %></td>
        <td><%= computer.masklan %></td>
        <td><%= computer.macwifi %></td>
        <td><%= computer.ipwifi %></td>
        <td><%= computer.maskwifi %></td>
        <td><%#= computer.operational %></td>
        <td><%#= computer.reazon %></td>
        <td><%= computer.Network %></td>
        <td><%= computer.Dependency %></td>
        <td><%= computer.Worker %></td>
        <td><%= computer.notes %></td>
        <td><%= link_to 'Show', computer %></td>
        <td><%= link_to 'Edit', edit_computer_path(computer) %></td>
        <td><%= link_to 'Destroy', computer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= button_to 'Nueva Computadora', new_computer_path, method: :get %>

<p id="notice"><%= notice %></p>

<h1>Listing Telephones</h1>

<table>
  <thead>
    <tr>
      <th>Ninventary</th>
      <th>Nserie</th>
      <th>Brand</th>
      <th>Model</th>
      <th>Buy date</th>
      <th>Genus</th>
      <th>Number</th>
      <th>Operational</th>
      <th>Reazon</th>
      <th>Notes</th>
      <th>Dependency</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @telephones.each do |telephone| %>
      <tr>
        <td><%= telephone.ninventary %></td>
        <td><%= telephone.nserie %></td>
        <td><%= telephone.brand %></td>
        <td><%= telephone.model %></td>
        <td><%= telephone.buy_date %></td>
        <td><%= telephone.genus %></td>
        <td><%= telephone.number %></td>
        <td><%= telephone.operational %></td>
        <td><%= telephone.reazon %></td>
        <td><%= telephone.notes %></td>
        <td><%= telephone.Dependency %></td>
        <td><%= link_to 'Show', telephone %></td>
        <td><%= link_to 'Edit', edit_telephone_path(telephone) %></td>
        <td><%= link_to 'Destroy', telephone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Telephone', new_telephone_path %>

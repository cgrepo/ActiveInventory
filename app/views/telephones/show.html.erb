<p id="notice"><%= notice %></p>
<div class="container-fluid">
  <div class="row">
    <div class="col-lg">
      <div class="box">
        <strong>No Inventario:</strong>
        <%= @telephone.ninventary %>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-lg">
      <div class="box">
        <strong>No serie:</strong>
        <%= @telephone.nserie %>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-lg">
      <div class="box">
         <strong>Marca:</strong>
        <%= @telephone.brand %>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-lg">
      <div class="box">
        <strong>Model:</strong>
        <%= @telephone.model %>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-lg">
      <div class="box">
        <strong>Tipo:</strong>
        <%= @telephone.genus %>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-lg">
      <div class="box">
        <strong>Fecha de Compra:</strong>
        <%= @telephone.buy_date %>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-lg">
      <div class="box">
        <strong>Numero:</strong>
        <%= @telephone.number %>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-lg">
      <div class="box">
         <strong>Notes:</strong>
        <%= @telephone.notes %>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-lg">
      <div class="box">
        <strong>Dependencia:</strong>
        <%= @telephone.Dependency.name %>
      </div>
    </div>
  </div>
</div>
<br>

<%= link_to 'Editar', edit_telephone_path(@telephone) %> |
<%= link_to 'Regresar', telephones_path %>

<p id="notice"><%= notice %></p>
<div class="container-fluid be-gray ">
  <div class="row">
    <div class="col-xs">
      <strong>Mes:</strong>
      <%= @statistic.month %>
    </div>
  </div>
  <div class="row">
    <div class="col-xs">      
      <strong>Hojas empleadas:</strong>
      <%= @statistic.sheets %>
    </div>
  </div>
  <div class="row">
    <div class="col-xs">      
      <strong>Consumible:</strong>
      <%= @statistic.consumable %>
    </div>
  </div>
  <div class="row">
    <div class="col-xs">      
        <% if @statistic.Copier %>
          <strong>Copier:</strong>
          <%= @statistic.Copier.model %>
        <% end %>
    </div>
  </div>
  <div class="row">
    <div class="col-xs">
        <% if @statistic.Printer %>
          <strong>Printer:</strong>
          <%= @statistic.Printer.model %>
        <% end %>
    </div>
  </div>
</div>
<br>
<%= link_to 'Editar', edit_statistic_path(@statistic), class:'btn btn-default btn-lg' %> |
<%= link_to 'Regresar', statistics_path, class:'btn btn-default btn-lg' %>
